{
  "name": "wenkudl",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.10.5",
  "description": "一些描述",
  "author": "LU-JIEJIE",
  "license": "MIT",
  "homepage": "https://github.com/LU-JIEJIE/wenkudl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LU-JIEJIE/wenkudl.git"
  },
  "bugs": "https://github.com/LU-JIEJIE/wenkudl/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.1.0",
    "@types/node": "^20.9.3",
    "bumpp": "^9.2.0",
    "eslint": "^8.54.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.1.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@lu-jiejie/utils": "^0.0.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
